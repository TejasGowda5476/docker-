pipeline{
    agent any
    stages{
        stage("for pull"){
            steps{
                sh "docker run -d --name dev httpd"
            }
        }
        stage("for awscli"){
            steps{
                sh"curl "https://awscli.amazonaws.com/awscli-exe-linux-x86_64.zip" -o "awscliv2.zip"
unzip awscliv2.zip
sudo ./aws/install"
            }
        }
    }
}
